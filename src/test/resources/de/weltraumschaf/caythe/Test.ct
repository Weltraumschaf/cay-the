
/*
 * Multiline comment.
 */

import foo.bar.Foo
import foo.bar.Bar
import foo.bar.Baz

public annotation AnnoFoo {}
package annotation AnnoBar {}
annotation AnnoBaz {}

public interface IfFoo {
    const String foo  = "FOO"
    const Integer bar = 42
    const Float baz   = 3.14
    /**
     * Method without return value.
     */
    foo()
    /**
     * Method with one return value.
     */
    String bar()
    /**
     * Method with two return values.
     */
    String, Integer baz()
}

package interface IfBar {}
interface IfBaz {}

public class Foo {
    public const String foo  = "FOO"
    package const Integer bar = 42
    const Float baz   = 3.14

    property String foo = "foobar"
    property (write) String bar
    property (readwrite) Integer baz = 42
}

package class Bar {}
class Baz {}