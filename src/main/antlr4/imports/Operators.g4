lexer grammar Operators;

OP_ASSIGN   : '='  ;
OP_EQUAL    : '==' ;
OP_STAR     : '*'  ;
OP_SLASH    : '/'  ;
OP_MINUS    : '-'  ;
OP_PLUS     : '+'  ;
OP_INC      : '++' ;
OP_DEC      : '--' ;
OP_LPAREN   : '('  ;
OP_RPAREN   : ')'  ;
OP_LCURLY   : '{'  ;
OP_RCURLY   : '}'  ;
OP_LBRACK   : '['  ;
OP_RBRACK   : ']'  ;
OP_LANGLE   : '<'  ;
OP_RANGLE   : '>'  ;